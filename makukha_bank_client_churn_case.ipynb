   "execution_count": 31,
   "metadata": {},
   "outputs": [],
   "source": [
    "#отдельно отложим тестовую выборку для финальной проверки перед sanity check\n",
    "features_test = data_test.drop(columns=['Exited', 'Gender', 'CustomerId', 'RowNumber', 'Surname'], axis=1)\n",
    "target_test = data_test['Exited']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "metadata": {},
   "outputs": [],
   "source": [
    "features_test = np.nan_to_num(features_test)\n",
    "target_test = np.nan_to_num(target_test)"
